{
  "name": "serverless-iamroles",
  "private": false,
  "version": "3.2.0",
  "engines": {
    "node": ">=12"
  },
  "description": "A Serverless plugin to define IAM Role statements as part of the function definition block",
  "main": "dist/lib/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test-bare": "npm run compile && mocha ./dist/test/**/*.test.js",
    "test": "nyc --reporter=lcovonly mocha --require ts-node/register --require source-map-support/register  ./src/test/**/*.test.ts",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "compile": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "npm run clean && npm run compile",
    "semantic-release": "semantic-release",
    "release": "standard-version",
    "lint": "eslint ."
  },
  "author": "Functional One, Ltd.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andersquist/serverless-iam-roles-per-function"
  },
  "keywords": [
    "aws",
    "lambda",
    "aws lambda",
    "serverless",
    "policy",
    "role",
    "iam",
    "custom",
    "permissions",
    "security"
  ],
  "dependencies": {
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@serverless/enterprise-plugin": "^4.1.2",
    "@types/chai": "^4.2.14",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "chai": "^4.2.0",
    "commitizen": "^4.2.4",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^10.0.0",
    "npm-get-version": "^1.0.2",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "semantic-release-contributors": "^3.1.1",
    "serverless": "^3.12.0",
    "source-map-support": "^0.5.19",
    "standard-version": "^9.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "serverless": "1 || 2 || 3"
  },
  "files": [
    "dist/index.*",
    "dist/lib/**",
    "src/",
    "*.md"
  ],
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/lib/**"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "standard-version": {
    "skip": {
      "tag": true,
      "commit": true
    }
  }
}
